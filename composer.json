{
    "name": "steelants/laravel-boilerplate-warehouse",
    "description": "Simple Warehouse Management System",
    "type": "library",
    "require": {
        "laravel/framework": "^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "steelants/boilerplate": "^2.3.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "SteelAnts\\LaravelBoilerplate\\Warehouse\\": "src/"
        }
    },
    "authors": [
        {
            "name": "SteelAnts",
            "email": "info@steelants.cz"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SteelAnts\\LaravelBoilerplate\\Warehouse\\BoilerplateWarehouseServiceProvider",
                "Barryvdh\\Debugbar\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Barryvdh\\Debugbar\\Facade"
            }
        }
    }
}
